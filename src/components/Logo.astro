---
import { Image } from 'astro:assets';
import logoImg from '~/assets/images/logo.webp';
---

<Image
  class="mx-auto w-full rounded-lg"
  width={50}
  height={50}
  widths={[50, 100]}
  sizes="(max-width: 640px) 50px, 100px"
  layout="responsive"
  loading="eager"
  alt="Logo of n3tz"
  src={logoImg}
/>
